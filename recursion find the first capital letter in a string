
            // recursion find the first capital letter in a string

#include <bits/stdc++.h>
using namespace std;

// int count=0;

void f(char *a, int n, int i){
    // base case
    if(a[i]>65 && a[i]<91){
        cout<<a[i]<<" ";
        return;
    }
    // rec case
    f(a,n,i+1);
}
int main() {

    char a[100];
    cin>>a;
    int n=strlen(a);
    int i=0;
    f(a,n,i);
    
}
