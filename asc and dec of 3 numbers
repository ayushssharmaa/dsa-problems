#include <iostream>
using namespace std;

int main() {

    int a,b,c;
    cin>>a>>b>>c;
   
    if((a>=b) && (a>=c))
    {
        if(b>=c)
        {
            cout<<"Ascending order is "<<a<<b<<c<<endl;
            cout<<"Decending order is "<<c<<b<<a<<endl;
        }
        else if(b<=c)
        {
            cout<<"Ascending order is "<<a<<c<<b<<endl;
            cout<<"Decending order is "<<b<<c<<a<<endl;
        }
    }
    else if((b>=a) && (b>=c))
    {
        if(a>=c) 
        {
            cout<<"Ascending order is "<<b<<a<<c<<endl;
            cout<<"Decending order is "<<c<<b<<a<<endl;
        }
        else if(a<=c) 
        {
            cout<<"Ascending order is "<<b<<c<<a<<endl;
            cout<<"Decending order is "<<a<<c<<b<<endl;
        }
    }
     else if((c>=a) && (c>=b))
    {
        if(a>=b) 
        {
            cout<<"Ascending order is "<<c<<a<<b<<endl;
            cout<<"Decending order is "<<c<<b<<a<<endl;
        }
        else if(a<=b) 
        {
            cout<<"Ascending order is "<<c<<a<<b<<endl;
            cout<<"Decending order is "<<b<<a<<c<<endl;
            
        }
    }

	return 0;
}
