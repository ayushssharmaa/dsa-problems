#include <iostream>
using namespace std;

int main() {

    int n,rem,sum=0;
    cin>>n;
    cout<<n<<endl;
    
    while(n>0){
        rem=n%10;
        sum=sum*10+rem;
        n=n/10;
    }
    cout<<sum<<endl;
    int temp=n;
    if(temp==n) cout<<"No is palendrome";
    else cout<<"No is not palendrome";

	return 0;
}
