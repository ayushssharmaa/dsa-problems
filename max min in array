#include <iostream>
using namespace std;

int main() {

    int a[20];
	int n,i;
	cin>>n;
	
	for(i=0; i<n; i++){
	    cin>>a[i];
	}
    for(i=0; i<n; i++){
	    cout<<a[i]<<" ";
	}
	cout<<endl;

    int max,min;
    max=min=a[0];
    
    for(i=0; i<n; i++)
    {
        if(a[i]>max)
        {
            max=a[i];
        }
        else if(a[i]<min)
        {
            min=a[i];
        }
    }
    cout<<max<<endl<<min;

	return 0;
}
