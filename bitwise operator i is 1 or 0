
        // i is 1 or 0 USING BITWISE OPERATORS

#include <iostream>
using namespace std;

int main() {

    int n,i;
    cin>>n>>i;
    int mask;
    mask=(1<<i);
    if((n&mask)>0){
        cout<<i<<"th bit is 1."<<endl;
    }
    else{
        cout<<i<<"th bit is 0."<<endl;
    }
    
	return 0;
}
