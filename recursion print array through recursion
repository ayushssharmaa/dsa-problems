
            // recursion print array through recursion

#include <iostream>
using namespace std;

void f(int *a, int n){
    if(n<0) return;
    f(a,n-1);
    cout<<a[n]<<" ";
}

int main() {
    
    int n;
    cin>>n;
    int a[n];
    for( int i=0;i<n;i++) cin>>a[i];
    f(a,n-1);
    
	return 0;
}
