#include <iostream>
using namespace std;

int main() {

    int a[20];
	int n,i;
	cin>>n;
	
	for(i=0; i<n; i++){
	    cin>>a[i];
	}
    for(i=0; i<n; i++){
	    cout<<a[i]<<" ";
	}
	cout<<endl;
	cout<<"Positive elements"<<endl;
	for(i=0; i<n; i++){
	    if(a[i]>0) cout<<a[i]<<" ";
	}
	cout<<endl<<"Negative elements"<<endl;
	for(i=0; i<n; i++){
	    if(a[i]<0) cout<<a[i]<<" ";
	}
    
	return 0;
}
