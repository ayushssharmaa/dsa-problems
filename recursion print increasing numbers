
            // recursion print increasing numbers
            
#include <iostream>
using namespace std;

void inc(int n){
    // base condition
    if(n==0){
        return;    
    }
    
    // recursive case 
    inc(n-1);
    cout<<n<<" ";
}

int main() {

    int n;
    cin>>n;
    inc(n);

	return 0;
}
