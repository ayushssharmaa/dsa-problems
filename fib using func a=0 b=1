#include <iostream>
using namespace std;

int fib(int n){
    int a=0,b=1,c=0;
    if(n==1) return 0;
    else if(n==2) return 1;
    else{
        for(int i=0;i<n-2;i++){
            c=a+b;
            a=b;
            b=c;
        }
        return c;
    }
}

int main() {

    int n;
    cin>>n;
    
    cout<<fib(n)<<"\n";

	return 0;
}
