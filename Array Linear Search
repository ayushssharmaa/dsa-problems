#include <iostream>
#include <climits>
using namespace std;

int main() {

    int n,i;
    int a[n];
    cin>>n;
    
    for(i=0;i<n;i++) cin>>a[i];
    int m;
    cin>>m;
    bool flag=0;
    for(i=0;i<n;i++) {
        if(a[i]==m) {
            flag=1;
            break;
        }
    }
    if(flag==1) cout<<i;
    else cout<<"-1";
    
    

	return 0;
}
