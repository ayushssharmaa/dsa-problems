#include <iostream>
using namespace std;

int main() {

    int i,j,m,n,a[5][5],b[5][5],c[5][5];
    cin>>m>>n;
    
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++) cin>>a[i][j];
    }
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++) cin>>b[i][j];
    }
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++) c[i][j]=a[i][j]+b[i][j];
    }
    
    cout<<"Resultant matrix is"<<endl;
    
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++) cout<<c[i][j]<<" ";
        cout<<endl;
    }

	return 0;
}
