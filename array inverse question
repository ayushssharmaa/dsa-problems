#include <bits/stdc++.h>
#include <iostream>
#include <climits>
using namespace std;

int main() {

    int n,i;
    cin>>n;
	int a[n];
    
    for(i=0;i<n;i++) cin>>a[i];
    
    int b[n]; 
    for(i=0;i<n;i++){
        b[a[i]]=i;
    }
    
    for(i=0;i<n;i++) cout<<b[i]<<" ";

    return 0;
}
