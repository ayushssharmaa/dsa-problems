#include <iostream>
using namespace std;

int main() {
    
    int i, j;
    
    for(i=1; i<=80; i++){
        cout<<"*";
    }
    cout<<"\n";
    for(i=2; i<=23; i++){
        cout<<"*";
        for(j=2; j<=79; j++){
            cout<<" ";
        }
        cout<<"*"<<"\n";
    }
    for(i=1; i<=80; i++){
        cout<<"*";
    }
    
	return 0;
}
