#include <iostream>
using namespace std;

int main() {
int i,j,m,n,a[5][5], sum=0;
    cin>>m>>n;
    
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++) cin>>a[i][j];
    }
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++) cout<<a[i][j]<<" ";
        cout<<endl;
    }
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++) sum=sum+a[i][j];
    }
    
    cout<<endl<<sum;
    
    return 0;
}
